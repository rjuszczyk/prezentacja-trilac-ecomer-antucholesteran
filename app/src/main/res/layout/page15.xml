<?xml version="1.0" encoding="utf-8"?>
<pl.pharmaway.prezentacjatrilac.view.FixedScaledLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:fixedWidth="1920dp"
    app:fixedHeight="1200dp"
    >

    <pl.pharmaway.prezentacjatrilac.view.FixedImageView
        android:layout_width="1920dp"
        android:layout_height="1200dp"
        app:imageReference="@drawable/page15"
        />

    <View
        android:layout_width="760dp"
        android:layout_height="470dp"
        android:background="#FFF"
        android:layout_marginLeft="220dp"
        android:layout_marginTop="250dp"
        android:id="@+id/p15_1"
        />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/p15_2">
        <View
            android:layout_width="850dp"
            android:layout_height="100dp"
            android:background="#FFF"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="750dp"

            />


        <View
            android:layout_width="690dp"
            android:layout_height="287dp"
            android:background="#FFF"
            android:layout_marginLeft="269dp"
            android:layout_marginTop="855dp"

            />
    </FrameLayout>

    <View
        android:layout_width="590dp"
        android:layout_height="500dp"
        android:background="#FFF"
        android:layout_marginLeft="1100dp"
        android:layout_marginTop="275dp"
        android:id="@+id/p15_3"
        />

    <pl.pharmaway.prezentacjatrilac.view.FixedImageView
        android:id="@+id/button_prev"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_gravity="left|center_vertical"
        />

    <pl.pharmaway.prezentacjatrilac.view.FixedImageView
        android:id="@+id/button_next"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_gravity="right|center_vertical"
        />

    <pl.pharmaway.prezentacjatrilac.view.FixedImageView
        android:id="@+id/button_sg"
        android:layout_width="300dp"
        android:layout_height="190dp"


        android:layout_gravity="left|bottom"
        />
</pl.pharmaway.prezentacjatrilac.view.FixedScaledLayout>